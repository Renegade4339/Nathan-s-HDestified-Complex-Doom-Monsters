ACTOR SentientExplosiveTracer : FastProjectile
{
Height 1
Radius 1
Speed 250
Scale 0.8
Damage 0
RenderStyle Add
Alpha 0.85
DeathSound "sentient/chaingunex"
DamageType "CerChainguns"
Decal DoomImpScorch
+FORCERADIUSDMG
+RANDOMIZE
+FORCEXYBILLBOARD
+NOTIMEFREEZE
States
{
Spawn:
    TRAC A 1 Bright
	Loop
Death:
	TNT1 A 0
	TNT1 A 0 A_SetTranslucent(0.75,1)
	TNT1 A 0 A_SpawnItem("SentientExplosiveEffect")
    MISL B 4 Bright A_Explode(Random(3,6)*5,72)
    MISL CD 3 Bright
    Stop
    }
}

ACTOR BabySentientExplosiveTracer : SentientExplosiveTracer
{
DamageType "Baby15K"
Scale 0.6
States
{
Spawn:
    TRAC A 1 Bright
	Loop
	}
}

ACTOR SentientExplosiveEffect
{
+CLIENTSIDEONLY
+NOGRAVITY
+NOBLOCKMAP
States
{
Spawn:
    TNT1 A 0
    TNT1 A 0 A_JumpIf(random(0,1) == 1, "Toaster")
	TNT1 A 0 A_SpawnItemEx("OrangeParticleSpawner", 0, 0, 0, 0, 0, 0, 0, 128) 
    TNT1 AAA 0 A_CustomMissile("Kaboom16", 0, 0, random(0,360), 2, random(0,360))
	TNT1 AAA 0 A_CustomMissile("SmokeFX6", 0, 0, random(0,360), 2, random(0,360))
	TNT1 A 1
	Stop
Toaster:
    TNT1 A 0
	Stop
	}
}

