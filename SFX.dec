ACTOR ProjectileBullet : FastProjectile
{
   Height 1
   Radius 1
   Speed 350
   Damage (Random(1,3)*5)
   +NOEXTREMEDEATH
   +NOTIMEFREEZE
   +BLOODSPLATTER
   Decal BulletChip
   States
   {
   Spawn:
      TNT1 A 1
      Loop
   Death:
      TNT1 A 0
      TNT1 A 1 A_SpawnItem("ModdedBulletPuff")
      Stop
   XDeath:
      TNT1 A 1
      Stop
	  }
}

ACTOR PlayerDTBFGRailgunSlug : ProjectileBullet
{
