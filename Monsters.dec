// This guy probably should just burstfire a ZM or something. 
// Maybe elite SMG Zombie instead?
ACTOR AssaultRifleZombie : HDmobbase 
{
Health 110
Radius 20
Height 56
Speed 8
Mass 100
PainChance 160
DropItem "HealthFlask" 35
DropItem "Stim" 35
DropItem "Clip" 40
MONSTER 
+FLOORCLIP
+MISSILEMORE



AttackSound "weapons/assaultrifle"
SeeSound "grunt/sight"
PainSound "grunt/pain"
DeathSound "grunt/death"
ActiveSound "grunt/active"
Obituary "%o was riddled by an Assault Rifle zombie."
Tag "U.A.Commando (Rifle)"
States
{
	Spawn:
			TNT1 A 0 Nodelay A_GiveInventory("GarrisonArmourWorn",random(20,80))
			ARZM A 10 A_HDLook
			ARII A 10 A_HDLook
			Loop
	See:
			ARZM AABB 3 A_HDChase
			TNT1 A 0 A_SpawnItem("FootStep",0,0,0,0)
			ARZM CCDD 3 A_HDChase
			TNT1 A 0 A_SpawnItem("FootStep",0,0,0,0)
			Loop
	Missile:
			TNT1 A 0 A_JumpIfCloser(750,"Close")
	Far:
			ARZM E 10 A_FaceTarget
			TNT1 A 0 A_SpawnItemEx("HDSpent9mm", 12, 6, 30, Random(2, 6), 0, Random(1, 3), Random(-80, -90), 128)
			ARZM F 2 Bright A_CustomBulletAttack(8.4,0,1,Random(2,6)*3)
			ARZM E 2 A_FaceTarget
			TNT1 A 0 A_SpawnItemEx("HDSpent9mm", 12, 6, 30, Random(2, 6), 0, Random(1, 3), Random(-80, -90), 128)
			ARZM F 2 Bright A_CustomBulletAttack(8.4,0,1,Random(2,6)*3)
			ARZM E 2 A_FaceTarget
			TNT1 A 0 A_SpawnItemEx("HDSpent9mm", 12, 6, 30, Random(2, 6), 0, Random(1, 3), Random(-80, -90), 128)
			ARZM F 2 Bright A_CustomBulletAttack(8.4,0,1,Random(2,6)*3)
			ARZM E 10 A_FaceTarget		
			TNT1 A 0 A_CPosRefire
			Goto Far+1
	Close:
			ARZM E 10 A_FaceTarget
			TNT1 A 0 A_SpawnItemEx("HDSpent9mm", 12, 6, 30, Random(2, 6), 0, Random(1, 3), Random(-80, -90), 128)
			ARZM F 2 Bright A_CPosAttack
			ARZM E 3 A_FaceTarget		
			TNT1 A 0 A_CPosRefire
			Goto Close+1
	Pain:
			ARZM G 6 A_Pain
			Goto See
	Death:
			ARZM H 5  
			ARZM I 5 A_Scream
			ARZM J 5 A_NoBlocking (0)
			ARZM K 5 A_SpawnItem("BodyCrash",0,0,0,0)
			ARZM L -1
			Stop
	XDeath:
			ARZM M 3
			ARZM N 3 A_XScream
			ARZM O 3 A_SpawnItemEx("GibbedGenerator",0,0,0,0,0,0,0,128,0)
			ARZM P 3 A_NoBlocking (0)
			ARZM QRST 3
			ARZM U -1
			Stop
	Raise:
			TNT1 A 0
			TNT1 A 0 A_SpawnItemEx("VileHealEffect",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
			ARZM KJIH 5
			Goto See
			}
}

ACTOR Chaingunner : HDMobBase 
{
  Obituary "%o was ripped to ribbons by a Chaingunner."
  Health 110
   Radius 20
  Height 56
  Mass 100
  Speed 8
  PainChance 140
  DropItem "BulletMag" 256 10
  Dropitem "Vulcanette"
  SeeSound "chainguy/sight"
  AttackSound "doublecger/fire"
  PainSound "chainguy/pain"
  DeathSound "chainguy/death"
  ActiveSound "chainguy/active"

  Tag "U.A.Commando (Chaingun)"
  Monster
  +FLOORCLIP
  +MISSILEMORE
  
  States
  {
  Spawn:
	TNT1 A 0 Nodelay A_GiveInventory("GarrisonArmourWorn",random(20,80))
    DPOS A 10 A_hdLook
	DPOI A 10 A_hdLook
    Loop
  See:
    DPOS AABB 3 A_hdChase
    TNT1 A 0 A_SpawnItem("FootStep",0,0,0,0)
    DPOS CCDD 3 A_hdChase
    TNT1 A 0 A_SpawnItem("FootStep",0,0,0,0)
    Loop
  Missile:
	// Crude "I'm aiming" delay.
    DPOS E 10 A_FaceTarget
	
	// Volley
	DPOS E 4 Bright
	{
		A_Light("SHOT");
		A_StartSound("weapons/vulcanette",CHAN_WEAPON,CHANF_OVERLAP);
		A_CustomMissile("HDB_426Vulc",Random(30,34),0,Random(5,-5), CMF_OFFSETPITCH, random(-3, 3));
	}
	DPOS E 4 Bright
	{
		A_Light("SHOT");
		A_StartSound("weapons/vulcanette",CHAN_WEAPON,CHANF_OVERLAP);
		A_CustomMissile("HDB_426Vulc",Random(30,34),0,Random(5,-5), CMF_OFFSETPITCH, random(-3, 3));
    }
	DPOS E 4 Bright
	{
		A_Light("SHOT");
		A_StartSound("weapons/vulcanette",CHAN_WEAPON,CHANF_OVERLAP);
		A_CustomMissile("HDB_426Vulc",Random(30,34),0,Random(5,-5), CMF_OFFSETPITCH, random(-3, 3));
    }
	
	// Chance to stop firing.
	TNT1 A 0 A_Jump(100,"Pause")
	
	// "Should I fire again?"
    TNT1 A 0 A_CPosRefire
	// Loops.
    Goto Missile+1

	Pause:
		DPOS A 20
		goto see

  Pain:
    DPOS G 6 A_Pain
    Goto See
  Death:
    DPOS H 5
    DPOS I 5 A_Scream 
    DPOS J 5 A_NoBlocking (0) 
    DPOS K 5 A_SpawnItem("BodyCrash",0,0,0,0)
    DPOS L 5 
	DPOS M -1
    Stop
  XDeath:
    DPOS N 3
    DPOS O 3 A_XScream
    DPOS P 3 A_SpawnItemEx("GibbedGenerator",0,0,0,0,0,0,0,128,0)
    DPOS Q 3 A_NoBlocking (0)
	DPOS R 3
    DPOS S -1
	Stop
  Raise:
  	TNT1 A 0
	TNT1 A 0 A_SpawnItemEx("VileHealEffect",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
    DPOS RQPON 5
    Goto See
  }
}

